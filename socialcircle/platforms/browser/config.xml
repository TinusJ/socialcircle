<?xml version='1.0' encoding='utf-8'?>
<widget id="com.tktec.socialcircle" version="0.0.01" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>SocialCircle</name>
    <description>
        Some social stuff
    </description>
    <author email="jacksontinus96@gmail.com">
       Kyle,
	   Tinus
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
